{
  "name": "capsulajs-configuration-service",
  "version": "0.1.0",
  "description": "Capsula-JS Configuration Service",
  "main": "index.js",
  "repository": "https://github.com/capsulajs/capsulajs-configuration-service",
  "author": "Taras Shedenko <taras.shedenko@jivygroup.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:ts": "tsc",
    "build": "yarn build:ts",
    "test": "jest",
    "tsdoc": "rimraf docs/typescript && typedoc --mode file --module commonjs --jsx react --out ./docs/typescript --target es5 --ignoreCompilerErrors"
  },
  "jest": {
    "setupFiles": ["jest-localstorage-mock"],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "tests/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleDirectories": [
      "./"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/node": "^10.5.2",
    "flowgen": "^1.2.2",
    "jest": "^23.4.1",
    "jest-localstorage-mock": "^2.2.0",
    "ts-jest": "^23.0.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  }
}
